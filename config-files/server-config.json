{
    "mode": "single",
    "staticAddress": "server-pool",
    "servients": [
        {
            "instances": 1,
            "protocol": "http",
            "things": {
                "./examples/td/coffee_machine_td.json": {
                    "instances": 100,
                    "eventIntervals": {
                        "maintenance": 10,
                        "error": 8 
                    }
                }
            }  
        },
        {
            "instances": 1,
            "protocol": "coap",
            "things": {
                "./examples/td/coffee_machine_td.json": {
                    "instances": 500,
                    "eventIntervals": {
                        "maintenance": 5,
                        "error": 20
                    }
                }
            }
        },
        {
            "instances": 1,
            "protocol": "mqtt",
            "brokerURI":"test.mosquitto.org",
            "things": {
                "./examples/td/coffee_machine_td.json": {
                    "instances": 1,
                    "eventIntervals": {
                        "maintenance": 5,
                        "error": 20
                    }
                }
            }
        }

    ]
}
