{
    "title": "MinuteTickerThing",
    "processes": {
        "proc":{
            "dataMap": {
                "result": {},
                "array": {
                    "const": [1,2,3]
                }
            },
            "triggers": [{"runtimeEvent": "startup"}],
            "instructions": [
                {
                    "move": {
                        "from": {
                            "compound": [
                                "some string",        
                                { "copy": "./dmap/array" },        
                                { "copy": "length:./dmap/array" },
                                "Number of items in ${./dmap/array} is ${length:./dmap/array}"
                            ]    
                        },
                        "to": {
                            "pointer": "./dmap/result"
                        }
                    }
                },
                {"log": "$p4{./dmap/result}"}
            ]
        }
    }
}