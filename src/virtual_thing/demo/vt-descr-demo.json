{
    "dataMap": {
        "i": {
            "type": "number"
        },
        "test": {
            "type": "object",
            "default": {
                "someVal": 123
            }
        },
        "genericObject": {
            "type": "object"
        },
        "dtFormat": {
            "type": "string",
            "default": "unix"
        }
    },

    "processes": {

        "testProcess": {
            "instructions": [                
                {
                    "move": {
                        "from": {
                            "compound": {
                                "someProp": "somestr",
                                "someNum": 123,
                                "dates": [
                                    "${/dt/local}",
                                    "${/dt/utc}",
                                    {"pointer": "/dt/${/dmap/dtFormat}"}
                                ]
                            }
                        },
                        "to": {
                            "pointer": "/dmap/genericObject"
                        }
                    }
                },
                { "log": { "expression": "Value of /dmap/genericObject/dates/2:  ${/dmap/genericObject/dates/2}" } }
            ]
        },
        

        "testProcess2": {
            "instructions": [
                {
                    "log": { "expression": "${/dt/local}" }                    
                },
                {
                    "delay": "2s",
                    "log": { "expression": "${/dt/local}" }                    
                },
                {
                    "delay": "3s",
                    "log": { "expression": "${/dt/local}" }                    
                },
                {
                    "delay": "2s",
                    "log": { "expression": "${/dt/local}" }                    
                }
            ]
        },

        "testProcess1": {
            "instructions": [
                {
                    "delay": "2s",
                    "loop": {
                        "iterator": "/dmap/i",
                        "initialValueExpr": { "mathjs": "10" },
                        "condition": { "mathjs": "${/dmap/i} <= 20" },
                        "increment": 2,
                        "rate": { "mathjs": "1" },
                        "instructions": [
                            {
                                "log": { "expression": "${/dt/local} Value of i: ${/dmap/i}" }
                            }
                        ]
                    }
                }
            ]
        }
    }
}