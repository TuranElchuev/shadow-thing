{
    "dataMap": {
        "myObj": {
            "type": "number"
        }        
    }, 

    "processes": {
        
        "testAll": {
            "triggers": [
                { "runtimeEvent": "startup" }
            ],
            "instructions": [
                { "invokeProcess": "/proc/testMove" }
            ]
        },

        "testMove": {

            "dataMap": {
                "num": { "type": "number" }
            },

            "instructions": [
                {
                    "move": {
                        "from": {
                            "compound": {
                                "str": "${/dt/local}"
                            }
                        },
                        "to": { "pointer": "/dmap/myObj" }
                    }
                },
                { "log": "/dmap/myObj after: ${/dmap/myObj}\n" }
            ]
        }
    }
}