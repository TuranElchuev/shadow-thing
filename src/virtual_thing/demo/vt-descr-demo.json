    "dataMap": {
        "myObj": {
            "type": "object",
            "properties": {
                "num": { "type": "number" }
            }
        }        
    }, 

    "processes": {
        
        "testAll": {
            "triggers": [
                { "runtimeEvent": "startup" },
                { "rate": { "expression": "0.3" } }
            ],
            "instructions": [
                { "invokeProcess": "/proc/testMove" }
            ]
        },

        "testMove": {

            "dataMap": {
                "num": { "type": "number" }
            },

            "instructions": [
                {
                    "move": {
                        "from": {
                            "compound": 5
                        },
                        "to": { "pointer": "/dmap/myObj/num" }
                    }
                },
                { "log": "/dmap/myObj after: ${/dmap/myObj/num}\n" }
            ]
        }
    }
