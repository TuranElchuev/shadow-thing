{  
    "processes": {
        "testStringConcat": {
            "dataMap": {
                "str": {
                    "type": "string"
                },
                "obj": {
                    "type": "object",
                    "properties": {
                        "a": { "type": "number", "default": 5 },
                        "b": { "type": "string", "default": "str" },
                        "c": { "type": "array", "items": {"type": "number"}, "default": [1,2,3] }
                    }
                },
                "obj2": {
                    "type": "object"
                }
            },
            "triggers": [
                { "runtimeEvent": "startup" }
            ],
            "instructions": [
                {
                    "move": {
                        "from": {
                            "compound": "${/proc/testStringConcat/dmap/obj}"
                        },
                        "to": {
                            "pointer": "/proc/testStringConcat/dmap/str"
                        }
                    }
                },
                {
                    "log": "${/proc/testStringConcat/dmap/str}"
                }
            ]
        }
    }
}